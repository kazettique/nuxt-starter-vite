{
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll": false,
    "source.fixAll.eslint": true
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "files.eol": "\r\n",
  "cSpell.words": [
    "Nuxt",
    "pinia",
    "tailwindcss"
  ]
}
